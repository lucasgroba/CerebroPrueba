@model IEnumerable<SHARE.Entities.Usuario>

@{
    ViewBag.Title = "Index";
}

<h2>Roles de Usuarios</h2>


<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.mail)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Tipo_User)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.EmpresaRef)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.mail)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Tipo_User)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.EmpresaRef)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.Id }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.Id })
        </td>
    </tr>
}

</table>
